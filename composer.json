{
  "name": "thecodingmachine/symfony-console-universal-module",
  "type": "library",
  "description": "Cross-framework module for symfony/console.",
  "keywords": ["symfony", "console", "service provider"],
  "license": "MIT",
  "authors": [
    {
      "name": "Xavier HUBERTY"
    },
    {
      "name": "David NEGRIER",
      "email": "d.negrier@thecodingmachine.com"
    }

  ],
  "require": {
    "php": ">=7.0",
    "thecodingmachine/funky": "^1",
    "symfony/console": "^3 || ^4",
    "symfony/event-dispatcher": "^3 || ^4"
  },
  "autoload": {
    "psr-4": {
      "TheCodingMachine\\": "src/"
    }
  },
  "bin": [ "bin/app_console" ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
